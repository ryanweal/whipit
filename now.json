{
  "version": 2,
  "builds": [{
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "TWILIO_SID": "@whipit-twilio-sid",
    "TWILIO_TOKEN": "@whipit-twilio-token",
    "TWILIO_FROM": "@whipit-twilio-from",
    "USERS": "@whipit-users"
  }
}